{
    "name": "VAMK dark mode",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Dark theme for VAMK portal",
    "content_scripts": [
        {
            "js": ["init.js"],
            "css": ["dark.css"],
            "matches": ["*://portal.vamk.fi/*"]
        }
    ],
    "web_accessible_resources": ["dark.css"]

}