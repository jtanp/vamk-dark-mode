{
    "name": "VAMK dark mode",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Dark theme for VAMK portal and schedule.",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "css": ["darkPortal.css"],
            "matches": ["*://portal.vamk.fi/*"]
        },
        {
            "css": ["darkLukkari.css"],
            "matches": ["*://lukkarit.vamk.fi/*"]
        }
    ]
}