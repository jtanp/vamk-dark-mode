{
    "name": "VAMK dark mode",
    "version": "0.2",
    "manifest_version": 2,
    "description": "Dark theme for VAMK portal.",
    "content_scripts": [
        {
            "css": ["darkPortal.css"],
            "matches": ["*://portal.vamk.fi/*"]
        },
        {
            "css": ["darkLukkari.css"],
            "matches": ["*://lukkarit.vamk.fi/*"]
        }
    ]
}